<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-c2d">
        <sub-class-of type="application/octet-stream"/>

        <_comment>WinOnCD image file</_comment>

        <glob pattern="*.c2d"/>

        <magic priority="80">
             <match value="Adaptec CeQuadrat VirtualCD File" type="string" offset="0"/>
             <match value="Roxio Image File Format 3.0" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
