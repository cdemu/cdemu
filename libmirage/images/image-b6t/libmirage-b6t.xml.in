<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
    <mime-type type="application/x-b6t">
        <sub-class-of type="application/octet-stream"/>

        <_comment>BlindWrite 5/6 image file</_comment>

        <glob pattern="*.b5t"/>
        <glob pattern="*.b6t"/>

        <magic priority="80">
            <match value="BWT5 STREAM SIGN" type="string" offset="0"/>
        </magic>
    </mime-type>
</mime-info>
