#!/bin/sh
# postinst script for cdemu-daemon
#

myerror ()
{
	echo "ERROR: $1" >&2
	exit 1
}

mywarning ()
{
	echo "WARNING: $1" >&2
}

case "$1" in
	configure|abort-upgrade|abort-remove|abort-deconfigure)
	;;

	*)
		myerror "postinst called with unknown argument '$1'"
	;;
esac

#DEBHELPER#

exit 0
