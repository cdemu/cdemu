<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "https://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
 <interface name="org.freedesktop.login1.Manager">
  <method name="Inhibit">
   <arg type="s" name="what" direction="in"/>
   <arg type="s" name="who" direction="in"/>
   <arg type="s" name="why" direction="in"/>
   <arg type="s" name="mode" direction="in"/>
   <arg type="h" name="pipe_fd" direction="out"/>
  </method>
  <signal name="PrepareForSleep">
   <arg type="b" name="start"/>
  </signal>
 </interface>
</node>
