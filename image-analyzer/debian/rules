#!/usr/bin/make -f
# -*- makefile -*-


#export DH_VERBOSE=1


override_dh_auto_configure:
	dh_auto_configure -- --enable-gtk3

override_dh_strip:
	dh_strip --dbg-package="image-analyzer-dbg"

build:
	-test -x autogen.sh && ./autogen.sh
	dh build --with autotools_dev,apport

%:
	dh $@ --with autotools_dev,apport

